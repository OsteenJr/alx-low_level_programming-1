#include <stdio.h>

/**
 * swap_int - Swaps the value of two integers.
 * @a: the first integer to be swapped.
 * @b: The second integer to be swapped
 *
 * Rturn : 0
 */

void swap_int(int *a, int *b)
/* the function that swaps the values of two integers. */
{
	int temp = *a;
	*a = *b;
	*b = temp;
}
